{
  "compilerOptions": {
    "outDir": "/dist",
    "types": ["blockbench-types", "node"],
    "module": "node16",
    "target": "es2022",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node16",
    "allowJs": true,
    "strictNullChecks": true,
		"resolveJsonModule": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "node_modules"
  ]
}